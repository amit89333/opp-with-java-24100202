public class SwapWithThirdVariable {
    public static void main(String[] args) {
        int a = 10;
        int b = 25;

        System.out.println("Before swapping:");
        System.out.println("a = " + a + ", b = " + b);

        int temp = a;
        a = b;
        b = temp;

        System.out.println("After swapping using third variable:");
        System.out.println("a = " + a + ", b = " + b);
    }
}
