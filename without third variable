public class SwapWithoutThirdVariable {
    public static void main(String[] args) {
        int a = 10;
        int b = 25;

        System.out.println("Before swapping:");
        System.out.println("a = " + a + ", b = " + b);

        a = a + b;
        b = a - b;
        a = a - b;

        System.out.println("After swapping without using third variable:");
        System.out.println("a = " + a + ", b = " + b);
    }
}
